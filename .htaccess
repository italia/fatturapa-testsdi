AddHandler application/x-httpd-php70 .php .php5 .php4 .php3

RewriteEngine on

RewriteRule ^sdi/rpc/js/(.*)$ rpc/packages/fatturapa/ui/src/public/js/$1 [L]
RewriteRule ^sdi/rpc/css/(.*)$ rpc/packages/fatturapa/ui/src/public/css/$1 [L]
RewriteRule ^sdi/rpc/webfonts/(.*)$ rpc/packages/fatturapa/ui/src/public/webfonts/$1 [L]
RewriteRule ^sdi/rpc/font/(.*)$ rpc/packages/fatturapa/ui/src/public/font/$1 [L]
RewriteRule ^(.*)/rpc/(.*)/ /rpc/public/$2 [QSA,L]
RewriteRule ^(.*)/rpc/(.*) /rpc/public/$2 [QSA,L]
RewriteRule ^(.*)/soap/(.*)/ /soap/index.php [L]
RewriteRule ^(.*)/soap/(.*) /soap/index.php [L]

